{
  "name": "blog",
  "version": "2.3.0",
  "scripts": {
    "ng": "ng",
    "coverage": "open ./coverage/blog/index.html",
    "start": "ng run blog:serve-ssr",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false",
    "compose": "python3 scripts/compose.py",
    "lint": "ng lint --fix",
    "format": "prettier-eslint --eslint-config-path ./.eslintrc --write '**/*.ts'",
    "prettier": "npx prettier --write .",
    "serve:ssr:blog": "node dist/blog/server/server.mjs",
    "build:ssr": "ng build --configuration=production",
    "serve:ssr": "node dist/blog/server/server.mjs",
    "prerender": "ng run blog:prerender",
    "dev:ssr": "ng run blog:serve-ssr"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.0.0",
    "@angular/cdk": "^20.0.1",
    "@angular/common": "^20.0.0",
