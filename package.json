{
  "name": "blog",
  "version": "2.3.1",
  "scripts": {
    "coverage": "open ./coverage/blog/index.html",
    "start": "ng serve",
    "build": "ng build --configuration=production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false",
